{
  "name": "mywebapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "webpack --mode production",
    "compile": "tsc",
    "predeploy": "yarn build",
    "deploy": "cdk deploy --app 'yarn aws-simple create'",
    "postdeploy": "aws-simple upload",
    "format": "prettier --write '**/*.{html,js,json,md,ts,tsx,yml}'",
    "format:test": "prettier --list-different '**/*.{html,js,json,md,ts,tsx,yml}'",
    "start": "aws-simple start",
    "test": "jest",
    "watch": "run-p start watch:build",
    "watch:build": "webpack --watch"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/react": "^9.4.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/webpack": "^4.41.6",
    "aws-cdk": "^1.24.0",
    "aws-simple": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-loader": "^7.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
